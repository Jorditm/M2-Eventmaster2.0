<header>
    <div class="nav-wrapper">
        <nav>
            <input class="hidden" type="checkbox" id="menuToggle">
            <label class="menu-btn" for="menuToggle">
                <div class="menu"></div>
                <div class="menu"></div>
                <div class="menu"></div>
            </label>
            <div class="nav-container">
                <ul class="nav-tabs">
                    {{#if isUserLoggedIn}}
                    <li class="nav-tab"><a href="/">Inicio</a></li>
                    <li id="subrallado-menú" class="nav-tab"><a href="/favoritos">Favoritos</a></li>
                    <li class="nav-tab"><a href="/event/event">Eventos</a></li>
                    <li class="nav-tab"><a href="/event/attractions">Artistas</a></li>
                    
                    <li class="nav-tab"><a href="/auth/logout">Cerrar Sesión</a></li>
                    {{else}}
                    <li class="nav-tab"><a href="/auth/signup">Registrarse</a></li>
                    <li class="nav-tab"><a href="/auth/login">Iniciar Sesión</a></li>
                    {{/if}}
                </ul>
            </div>
        </nav>
    </div>
</header>
<main>
<div class="resultados-fav">
    {{#each favoritos}}
    <div class="etiqueta-fav">
        <img src="{{this.images}}" alt="">
        <div class="texto-favorito">
            <div class="titulo">
                <h2> {{this.name}}</h2>
            </div>
            <div class="fecha">
                <h4>{{this.localDate}}</h4>
            </div>
            <div class="url">
                <a class="primary" href="{{this.url}}">Tickets</a>
            </div>
        </div>

    </div>
    {{/each}}
</div>
</main>