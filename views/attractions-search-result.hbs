<header>
    <div class="nav-wrapper">
        <nav>
            <input class="hidden" type="checkbox" id="menuToggle">
            <label class="menu-btn" for="menuToggle">
                <div class="menu"></div>
                <div class="menu"></div>
                <div class="menu"></div>
            </label>
            <div class="nav-container">
                <ul class="nav-tabs">
                    {{#if isUserLoggedIn}}
                    <li class="nav-tab"><a href="/">Inicio</a></li>
                    <li class="nav-tab"><a href="/favoritos">Favoritos</a></li>
                    <li class="nav-tab"><a href="/event/event">Eventos</a></li>
                    <li id="subrallado-menú" class="nav-tab"><a href="/event/attractions">Artistas</a></li>
                    
                    <li class="nav-tab"><a href="/auth/logout">Cerrar Sesión</a></li>
                    {{else}}
                    <li class="nav-tab"><a href="/auth/signup">Registrarse</a></li>
                    <li class="nav-tab"><a href="/auth/login">Iniciar Sesión</a></li>
                    {{/if}}
                </ul>
            </div>
        </nav>
    </div>
</header>
<main>
<div class="resultados">
    {{#each events}}
    <div class="etiqueta">
        <img src="{{this.images.6.url}}" alt="">
        <div class="texto-search-artist">
            <div class="titulo-search-artist">
                <h2> {{this.name}}</h2>
            </div>
            <div class="redes">
            <a class="youtube" href="{{this.externalLinks.youtube.0.url}}"></a>
            <a class="twitter" href="{{this.externalLinks.twitter.0.url}}"></a>
            <a class="itunes" href="{{this.externalLinks.itunes.0.url}}"></a>
            <a class="instagram" href="{{this.externalLinks.instagram.0.url}}"></a>
            </div>
            <div class="url">
                <a class="primary" href="{{this.url}}">Tickets</a>
            </div>
        </div>
    </div>
    {{/each}}
</div>
</main>